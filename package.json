{
    "name": "baal-syntax-highlight",
    "displayName": "baal syntax highlight",
    "description": "baal syntax highlight",
    "version": "1.0.0",
    "publisher": "quarter",
    "engines": {
        "vscode": "^1.47.0"
    },
    "scripts": {
        "build": "vsce package"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "baal",
            "aliases": ["baal"],
            "extensions": [".faced"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "baal",
            "scopeName": "source.baal",
            "path": "./syntaxes/baal.tmLanguage.json",
            "tokenTypes": {
                "entity.name.namespace.baal": "other"
            }
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/quartorz/vscode-baal-syntax-highlight.git"
    }
}